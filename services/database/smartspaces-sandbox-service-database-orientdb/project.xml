<?xml version="1.0"?>
<project type="library" language="java">
  <name>OrientDB Database Service</name>
  <description>
    A service for working with OrientDB databases.
  </description>

  <identifyingName>
    smartspaces.sandbox.service.database.document.orientdb
  </identifyingName>
  <version>1.0.0</version>

  <library>
    <container>
      <activator>
        io.smartspaces.sandbox.service.database.document.orientdb.internal.osgi.OrientDbDocumentDatabaseServiceOsgiBundleActivator
      </activator>
    </container>
  </library>

  <dependencies>
    <dependency identifyingName="com.orientechnologies.orientdb-core"
        version="[2.2.0, 2.3.0)" required="true" />
    <dependency identifyingName="com.orientechnologies.orientdb-server"
        version="[2.2.0, 2.3.0)" required="true" />
    <dependency identifyingName="com.orientechnologies.orientdb-client"
        version="[2.2.0, 2.3.0)" required="true" />
    <dependency identifyingName="com.orientechnologies.orientdb-tools"
        version="[2.2.0, 2.3.0)" required="true" />
  </dependencies>
</project>
